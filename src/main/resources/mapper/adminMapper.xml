<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="bit.data.dao.AdminDao">
<!--	 유저 수-->
	<select id="getTotalCount" resultType="int">
		select count(*) from user
	</select>

<!--	코스수-->
	<select id="selectCafeTotalCount" parameterType="Map" resultType="int">
		select count(*) from cafe
	</select>

	<select id="selectFoodTotalCount" parameterType="Map" resultType="int">
		select count(*) from food
	</select>

	<select id="selectTripTotalCount" parameterType="Map" resultType="int">
		select count(*) from trip
	</select>
	<!--    문의사항-->
	<select id="getAllData" resultType="QnaDto">
		select * from qna order by qna_num desc
	</select>







	
</mapper>






















